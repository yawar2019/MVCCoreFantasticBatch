@model EmpDept
@{
    ViewData["Title"] = "GetMultipleDataView3";
}

<h1>GetMultipleDataView3</h1>

<table>
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
    </tr>
    @foreach(var item in Model.Emp)
    {
    <tr>
        <td>@item.EmpId</td>
        <td>@item.EmpName</td>
        <td>@item.EmpSalary</td>
    </tr>
    }
</table>

<br />

<table>
    <tr>
        <th>DeptId</th>
        <th>DeptName</th>
    </tr>
    @foreach(var item in Model.dept)
    {
    <tr>
        <td>@item.DeptId</td>
        <td>@item.DeptName</td>
    </tr>
    }
</table>


